// Grammaire PEST pour VOLMET (Meteorological Information for Aircraft in Flight)

volmet = {
    "VOLMET" ~ space+ ~ volmet_content
}

volmet_content = {
    (!"\n" ~ !"\r" ~ printable_char)*
}

letter = {
    'A'..'Z' | 'a'..'z'
}

digit = {
    '0'..'9'
}

space = {
    " "
}

printable_char = {
    '\x20'..'\x7E' | '\x80'..'\xFF'
}

