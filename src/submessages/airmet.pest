// Grammaire PEST pour AIRMET (Airmen's Meteorological Information)

airmet = {
    "AIRMET" ~ space+ ~ airmet_content
}

airmet_content = {
    (!"\n" ~ !"\r" ~ printable_char)*
}

letter = {
    'A'..'Z' | 'a'..'z'
}

digit = {
    '0'..'9'
}

space = {
    " "
}

printable_char = {
    '\x20'..'\x7E' | '\x80'..'\xFF'
}

