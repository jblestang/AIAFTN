// Grammaire PEST pour ATIS (Automatic Terminal Information Service)

atis = {
    "ATIS" ~ space+ ~ atis_content
}

atis_content = {
    (!"\n" ~ !"\r" ~ printable_char)*
}

letter = {
    'A'..'Z' | 'a'..'z'
}

digit = {
    '0'..'9'
}

space = {
    " "
}

printable_char = {
    '\x20'..'\x7E' | '\x80'..'\xFF'
}

